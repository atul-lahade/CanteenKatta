<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="100dp"
    tools:context="com.example.root.canteenkatta.Options"
    android:background="@drawable/canteen"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center_horizontal">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SP JAIN MESS"
            android:layout_gravity="center_horizontal"
            android:id="@+id/jain_mess_button"
            android:background="@null"/>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SPCE HOSTEL MESS"
            android:id="@+id/hostel_mess_button" />


        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SP JAIN MESS HOTEL"
            android:id="@+id/jain_hotel_button" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SPCE CANTEEN"
            android:layout_gravity="center_horizontal"
            android:id="@+id/spce_canteen_button" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="JP JAIN CANTEEN"
            android:layout_gravity="center_horizontal"
            android:id="@+id/jain_canteen_button" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="BHAVANS CANTEEN"
            android:layout_gravity="center_horizontal"
            android:id="@+id/bhavans_canteen_button" />

    </LinearLayout>
</LinearLayout>
